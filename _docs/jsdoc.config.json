{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./public", "./routes", "./server.js"],
    "exclude": ["./node_modules", "./__tests__", "./jest.config.js", "./_docs", "./assets"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": true,
    "destination": "./_docs",
    "template": "./assets/docdash"
  }
}
