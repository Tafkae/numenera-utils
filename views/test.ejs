<!DOCTYPE html>
<html lang="en">


  <head>
    <%- include('head.ejs') %>
  </head>
  <body>
    <%- include('nav-primary.ejs') %>
    <h1>Test utils functions</h1>

    <section class="block-element" id="test-savelocal">
      <h2>saveLocal()</h2>

      <h3 class="char-name"></h3>
      <div class="char-summary"></div>

      <button type="button" id="btn-save-local">Save Local</button>
      <div class="message hidden"></div>
    </section>

    <section class="block-element" id="test-loadlocal">
      <h2>loadLocal()</h2>

      <h3 class="char-name"></h3>
      <div class="char-summary"></div>

      <button type="button" id="btn-load-local">Load from Local</button>
      <div class="message hidden"></div>
    </section>

    <section class="block-element" id="test-parse">
      <h2>tryParseJsonObj()</h2>

      <h3 class="char-name"></h3>
      <div class="char-summary"></div>

      <button type="button" id="btn-parse">Parse it</button>
      <div class="message hidden"></div>
    </section>

    <section class="block-element" id="test-msgHandler">
      <h2>MessageElementHandler class</h2>

      <h3 class="char-name"></h3>
      <div class="char-summary"></div>

      <button type="button" id="btn-mh-success">Succeed</button> <button type="button" id="btn-mh-failure">Fail</button>
      <div class="message hidden"></div>
    </section>

    <section class="block-element" id="test-rpga">
      <h2>RPG Awesome font</h2>
      <i class="ra ra-sword ra-2x"></i>
    </section>

    <script src="/scripts/test.js" type="module"></script>
  </body>
</html>
